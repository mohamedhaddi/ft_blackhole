{
    "manifest_version": 2,
    "name": "ft_logtime",
    "version": "2.0",
    "description": "Calculates your logtime at https://intra.42.fr.",
    "icons": {
        "48": "icons/42-logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://profile.intra.42.fr/",
                "https://profile.intra.42.fr/users/*",
                "https://profile.intra.42.fr/users/*/"
            ],
            "js": ["ft_logtime.js"]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "ft_logtime-2.0@intra.42.fr"
        }
    }
}
